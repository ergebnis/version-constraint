{
  "name": "ergebnis/version-constraint",
  "description": "Provides abstractions of version constraints.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "version",
    "constraint"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/version-constraint",
  "support": {
    "issues": "https://github.com/ergebnis/version-constraint/issues",
    "source": "https://github.com/ergebnis/version-constraint",
    "security": "https://github.com/ergebnis/version-constraint/blob/main/.github/SECURITY.md"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0"
  },
  "require-dev": {
    "composer/semver": "^3.4.3",
    "ergebnis/composer-normalize": "^2.45.0",
    "ergebnis/data-generator": "^1.1.0",
    "ergebnis/data-provider": "^3.4.0",
    "ergebnis/license": "^2.6.0",
    "ergebnis/php-cs-fixer-config": "^6.46.0",
    "ergebnis/phpunit-slow-test-detector": "^2.19.0",
    "fakerphp/faker": "^1.24.1",
    "infection/infection": "~0.27.11",
    "phpunit/phpunit": "^10.5.27",
    "psalm/plugin-phpunit": "~0.19.0",
    "rector/rector": "^2.0.11",
    "roave/backward-compatibility-check": "^8.6.0",
    "vimeo/psalm": "^5.26.1"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\VersionConstraint\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\VersionConstraint\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "8.1.26"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
